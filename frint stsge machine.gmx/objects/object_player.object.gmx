        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Controller
// State Controller
switch (state) {
    case (player_states.idle):
        script_player_idle ();
        break;
    case (player_states.move):
        script_player_move ();
        break;
    case (player_states.jump):
        script_player_jump ();
        break;
    case (player_states.roll):
        script_player_roll ();
        break;
    case (player_states.crouch):
        script_player_crouch ();
        break;
}

//Physic
if (moveY &lt; 10) {
    moveY = min(moveY + grav, 9)
}

//Collision Check
if (place_meeting(x + moveX, y,object1_ground)) {
    repeat(abs(moveX)) {  
        if (not place_meeting (x +sign (moveX),y, object1_ground))  {
            x += sign(moveX)
        }
    
    moveX = 0
    }
}
if (place_meeting(x , y + moveY, object1_ground)) {
    repeat(abs(moveY)) {
        if (not place_meeting(x,y + sign(moveY), object1_ground))
                y += sign(moveY)
        }
        
        
    moveY = 0
}
if (place_meeting(x + moveX, y,object_dirt_block)) {
    repeat(abs(moveX)) {  
        if (not place_meeting (x +sign (moveX),y, object_dirt_block))  {
            x += sign(moveX)
        }
    
    moveX = 0
    }
}
if (place_meeting(x , y + moveY, object_dirt_block)) {
    repeat(abs(moveY)) {
        if (not place_meeting(x,y + sign(moveY), object_dirt_block))
                y += sign(moveY)
        }
        
        
    moveY = 0
}

//Sprite Check
if (input_left + input_right != 0 and  input_left + input_right !=2) {
    image_xscale = sign(input_right - input_left)
}

//Shoot
if input_shoot {
    grenade = instance_create(x,y,object54_grenade)
    if image_xscale &gt; 0 {
        grenade.direction = 0
    }
    if image_xscale &lt; 0 {
        grenade.direction = 180
    }
    grenade.image_xscale = image_xscale
    grenade.speed = 5
}

//Apply Movement
x += moveX
y += moveY
</string>
